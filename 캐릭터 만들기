function setup() {
  createCanvas(400, 400);
}

function draw() {
  fill(255,0,0);
  rect(60,30,10,10);
  fill(255,255,255);
  square(40,40,50,10);
  fill(0,0,0);
  rect(50,50,10,10);
  fill(0,0,0);
  rect(70,50,10,10);
  fill(255,255,0);
  rect(53,70,23,7);
  fill(255,255,0);
  rect(53,78,23,7);
  fill(255,0,0);
  rect(43,65,5,5);
  fill(255,0,0);
  rect(80,65,5,5);
}
